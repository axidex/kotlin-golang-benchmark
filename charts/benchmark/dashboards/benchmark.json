{
  "uid": "benchmark-performance-k8s",
  "title": "Benchmark Performance (Quarkus vs Golang) - k8s",
  "tags": ["benchmark", "kubernetes"],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "5s",
  "editable": true,
  "graphTooltip": 0,
  "time": { "from": "now-15m", "to": "now" },
  "timepicker": { "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"] },
  "templating": {
    "list": [
      {
        "type": "datasource",
        "name": "datasource",
        "label": "Prometheus",
        "query": "prometheus",
        "current": { "selected": false, "text": "Prometheus", "value": "Prometheus" }
      },
      {
        "type": "query",
        "name": "namespace",
        "label": "Namespace",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": { "query": "label_values(up, namespace)", "refId": "ns" },
        "sort": 1,
        "refresh": 2,
        "current": { "selected": false, "text": "benchmark", "value": "benchmark" }
      },
      {
        "type": "query",
        "name": "quarkus_service",
        "label": "Quarkus service",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": { "query": "label_values(up{namespace=\"$namespace\"}, service)", "refId": "qs" },
        "regex": "/.*quarkus.*/",
        "sort": 1,
        "refresh": 2
      },
      {
        "type": "query",
        "name": "golang_service",
        "label": "Golang service",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": { "query": "label_values(up{namespace=\"$namespace\"}, service)", "refId": "gs" },
        "regex": "/.*golang.*/",
        "sort": 1,
        "refresh": 2
      },
      {
        "type": "query",
        "name": "quarkus_pod",
        "label": "Quarkus pod",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": { "query": "label_values(up{namespace=\"$namespace\",service=\"$quarkus_service\"}, pod)", "refId": "qpod" },
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 2,
        "sort": 1
      },
      {
        "type": "query",
        "name": "golang_pod",
        "label": "Golang pod",
        "datasource": { "type": "prometheus", "uid": "${datasource}" },
        "query": { "query": "label_values(up{namespace=\"$namespace\",service=\"$golang_service\"}, pod)", "refId": "gpod" },
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 2,
        "sort": 1
      }
    ]
  },
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": { "type": "grafana", "uid": "-- Grafana --" },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Request Latency (p99, ms)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 50,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "quarkus" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "golang"  }, "properties": [{ "id": "color", "value": { "fixedColor": "green",  "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "calcs": ["lastNotNull"], "displayMode": "table", "placement": "right", "showLegend": true, "sortBy": "Last *", "sortDesc": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "1000 * histogram_quantile(0.99, sum by (le) (rate(http_server_requests_seconds_bucket{namespace=\"$namespace\",service=\"$quarkus_service\"}[1m])))",
          "legendFormat": "quarkus"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "1000 * histogram_quantile(0.99, sum by (le) (rate(http_request_duration_seconds_bucket{namespace=\"$namespace\",service=\"$golang_service\"}[1m])))",
          "legendFormat": "golang"
        }
      ]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "CPU Usage (cores)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 0 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 3,
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 50,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "quarkus" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "golang"  }, "properties": [{ "id": "color", "value": { "fixedColor": "green",  "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "right", "showLegend": true, "sortBy": "Last *", "sortDesc": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"$namespace\",pod=~\"$quarkus_pod\",container!=\"POD\",image!=\"\"}[1m]))",
          "legendFormat": "quarkus"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"$namespace\",pod=~\"$golang_pod\",container!=\"POD\",image!=\"\"}[1m]))",
          "legendFormat": "golang"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Requests Per Second (RPS)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 7 },
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 50,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "quarkus" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "golang"  }, "properties": [{ "id": "color", "value": { "fixedColor": "green",  "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "calcs": ["lastNotNull"], "displayMode": "table", "placement": "right", "showLegend": true, "sortBy": "Last *", "sortDesc": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum(rate(http_server_requests_seconds_count{namespace=\"$namespace\",service=\"$quarkus_service\"}[30s]))",
          "legendFormat": "quarkus"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "sum(rate(http_requests_total{namespace=\"$namespace\",service=\"$golang_service\"}[30s]))",
          "legendFormat": "golang"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Memory Usage (MB)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 7 },
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "decimals": 0,
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 50,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "quarkus" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "golang"  }, "properties": [{ "id": "color", "value": { "fixedColor": "green",  "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "right", "showLegend": true, "sortBy": "Last *", "sortDesc": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum(container_memory_working_set_bytes{namespace=\"$namespace\",pod=~\"$quarkus_pod\",container!=\"POD\",image!=\"\"}) / 1024 / 1024",
          "legendFormat": "quarkus"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "sum(container_memory_working_set_bytes{namespace=\"$namespace\",pod=~\"$golang_pod\",container!=\"POD\",image!=\"\"}) / 1024 / 1024",
          "legendFormat": "golang"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "CPU with Limits & Requests (cores)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "decimals": 3,
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byRegexp", "options": "/.*usage/" }, "properties": [{ "id": "custom.fillOpacity", "value": 50 }] },
          { "matcher": { "id": "byRegexp", "options": "/.*limit/" }, "properties": [{ "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } }] },
          { "matcher": { "id": "byRegexp", "options": "/.*request/" }, "properties": [{ "id": "custom.lineStyle", "value": { "fill": "dot", "dash": [2, 5] } }] }
        ]
      },
      "options": {
        "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "right", "showLegend": true, "sortBy": "Last *", "sortDesc": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"$namespace\",pod=~\"$quarkus_pod\",container!=\"POD\",image!=\"\"}[1m]))",
          "legendFormat": "quarkus usage"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_limits{namespace=\"$namespace\",pod=~\"$quarkus_pod\",resource=\"cpu\",unit=\"core\"})",
          "legendFormat": "quarkus limit"
        },
        {
          "refId": "C",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_requests{namespace=\"$namespace\",pod=~\"$quarkus_pod\",resource=\"cpu\",unit=\"core\"})",
          "legendFormat": "quarkus request"
        },
        {
          "refId": "D",
          "editorMode": "code",
          "expr": "sum(rate(container_cpu_usage_seconds_total{namespace=\"$namespace\",pod=~\"$golang_pod\",container!=\"POD\",image!=\"\"}[1m]))",
          "legendFormat": "golang usage"
        },
        {
          "refId": "E",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_limits{namespace=\"$namespace\",pod=~\"$golang_pod\",resource=\"cpu\",unit=\"core\"})",
          "legendFormat": "golang limit"
        },
        {
          "refId": "F",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_requests{namespace=\"$namespace\",pod=~\"$golang_pod\",resource=\"cpu\",unit=\"core\"})",
          "legendFormat": "golang request"
        }
      ]
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Memory with Limits & Requests (MB)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 7, "w": 12, "x": 12, "y": 14 },
      "fieldConfig": {
        "defaults": {
          "unit": "decmbytes",
          "decimals": 0,
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byRegexp", "options": "/.*usage/" }, "properties": [{ "id": "custom.fillOpacity", "value": 50 }] },
          { "matcher": { "id": "byRegexp", "options": "/.*limit/" }, "properties": [{ "id": "custom.lineStyle", "value": { "fill": "dash", "dash": [10, 10] } }] },
          { "matcher": { "id": "byRegexp", "options": "/.*request/" }, "properties": [{ "id": "custom.lineStyle", "value": { "fill": "dot", "dash": [2, 5] } }] }
        ]
      },
      "options": {
        "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "right", "showLegend": true, "sortBy": "Last *", "sortDesc": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "sum(container_memory_working_set_bytes{namespace=\"$namespace\",pod=~\"$quarkus_pod\",container!=\"POD\",image!=\"\"}) / 1024 / 1024",
          "legendFormat": "quarkus usage"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_limits{namespace=\"$namespace\",pod=~\"$quarkus_pod\",resource=\"memory\",unit=\"byte\"}) / 1024 / 1024",
          "legendFormat": "quarkus limit"
        },
        {
          "refId": "C",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_requests{namespace=\"$namespace\",pod=~\"$quarkus_pod\",resource=\"memory\",unit=\"byte\"}) / 1024 / 1024",
          "legendFormat": "quarkus request"
        },
        {
          "refId": "D",
          "editorMode": "code",
          "expr": "sum(container_memory_working_set_bytes{namespace=\"$namespace\",pod=~\"$golang_pod\",container!=\"POD\",image!=\"\"}) / 1024 / 1024",
          "legendFormat": "golang usage"
        },
        {
          "refId": "E",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_limits{namespace=\"$namespace\",pod=~\"$golang_pod\",resource=\"memory\",unit=\"byte\"}) / 1024 / 1024",
          "legendFormat": "golang limit"
        },
        {
          "refId": "F",
          "editorMode": "code",
          "expr": "sum(kube_pod_container_resource_requests{namespace=\"$namespace\",pod=~\"$golang_pod\",resource=\"memory\",unit=\"byte\"}) / 1024 / 1024",
          "legendFormat": "golang request"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "CPU Throttling (%)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 7, "w": 24, "x": 0, "y": 21 },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 2,
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "lineWidth": 2,
            "fillOpacity": 50,
            "showPoints": "auto"
          }
        },
        "overrides": [
          { "matcher": { "id": "byName", "options": "quarkus" }, "properties": [{ "id": "color", "value": { "fixedColor": "yellow", "mode": "fixed" } }] },
          { "matcher": { "id": "byName", "options": "golang"  }, "properties": [{ "id": "color", "value": { "fixedColor": "green",  "mode": "fixed" } }] }
        ]
      },
      "options": {
        "legend": { "calcs": ["lastNotNull", "max"], "displayMode": "table", "placement": "right", "showLegend": true, "sortBy": "Last *", "sortDesc": true },
        "tooltip": { "mode": "single", "sort": "none" }
      },
      "targets": [
        {
          "refId": "A",
          "editorMode": "code",
          "expr": "100 * sum(rate(container_cpu_cfs_throttled_seconds_total{namespace=\"$namespace\",pod=~\"$quarkus_pod\",container!=\"POD\",image!=\"\"}[1m])) / sum(rate(container_cpu_cfs_periods_total{namespace=\"$namespace\",pod=~\"$quarkus_pod\",container!=\"POD\",image!=\"\"}[1m]))",
          "legendFormat": "quarkus"
        },
        {
          "refId": "B",
          "editorMode": "code",
          "expr": "100 * sum(rate(container_cpu_cfs_throttled_seconds_total{namespace=\"$namespace\",pod=~\"$golang_pod\",container!=\"POD\",image!=\"\"}[1m])) / sum(rate(container_cpu_cfs_periods_total{namespace=\"$namespace\",pod=~\"$golang_pod\",container!=\"POD\",image!=\"\"}[1m]))",
          "legendFormat": "golang"
        }
      ]
    }
  ]
}
