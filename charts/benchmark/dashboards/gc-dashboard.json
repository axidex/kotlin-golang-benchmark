      ]
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Threads / Goroutines Count",
      "description": "Количество потоков (JVM) и горутин (Golang)",
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 58 },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": { "mode": "palette-classic" },
          "custom": {
            "drawStyle": "line",
